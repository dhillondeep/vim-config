{
    "diagnostic.errorSign": "✘",
    "diagnostic.warningSign": "",
    "diagnostic.infoSign": "",
    "solargraph.commandPath": "/usr/local/bin/solargraph",
    "solargraph.transport": "external",
    "solargraph.externalServer": {
        "host": "localhost",
        "port": 7658
    },
    "go.goplsPath": "/usr/local/google/home/ddhillon/Development/go/bin/gopls",
    "go.goplsOptions": {
        "directoryFilters": [
            "-bazel-bin",
            "-bazel-out",
            "-bazel-testlogs",
            "-bazel-mypkg"
        ],
        "gofumpt": true,
        "usePlaceholders": true,
        "semanticTokens": true
    },
    "go.goplsEnv": {
        "GOPACKAGESDRIVER": "/usr/local/google/home/ddhillon/go/src/ods/.vscode/gopackagesdriver.sh"
    }
}
